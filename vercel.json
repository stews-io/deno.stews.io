{
  "rewrites": [
    {
      "source": "/:toolkit_version([\\d]+.[\\d]+.[\\d]+)/command/:path*",
      "destination": "https://raw.githubusercontent.com/stews-io/toolkit/:toolkit_version/stew-command/:path*"
    },
    {
      "source": "/:toolkit_version([\\d]+.[\\d]+.[\\d]+)/:path*",
      "destination": "https://raw.githubusercontent.com/stews-io/toolkit/:toolkit_version/:path*"
    },
    {
      "source": "/command/:path*",
      "destination": "https://raw.githubusercontent.com/stews-io/toolkit/production/stew-command/:path*"
    },
    {
      "source": "/:path*",
      "destination": "https://raw.githubusercontent.com/stews-io/toolkit/production/:path*"
    }
  ]
}
